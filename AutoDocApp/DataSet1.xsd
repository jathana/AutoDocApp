            <MainSource>
              <DbSource ConnectionRef="QBCollection_Plus_23_1ConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
col.[name] as FIELD_NAME, 
OBJECT_NAME(col.object_id) as TABLE_NAME,
col.max_length,
col.precision,
col.scale,
CASE 
WHEN t.name='nvarchar' THEN t.name + '(' + CONVERT(NVARCHAR,col.max_length) + ')'
WHEN t.name='decimal' THEN t.name + '(' + CONVERT(NVARCHAR,col.precision) + ',' + CONVERT(NVARCHAR,col.scale) +')'
ELSE t.name
END as FIELD_DATATYPE,

tab.tab_prefix
FROM sys.columns col
INNER JOIN sys.types t ON col.user_type_id = t.user_type_id
INNER JOIN Tables tab with(nolock) ON tab.tab_name=OBJECT_NAME(col.object_id)
WHERE 
   col.object_id = OBJECT_ID('AT_CASE_AGGREGATIONS')
OR col.object_id = OBJECT_ID('AT_CASE_BILLING')
OR col.object_id = OBJECT_ID('AT_CASE_CORE')
OR col.object_id = OBJECT_ID('AT_CASE_EXTRA')
OR col.object_id = OBJECT_ID('AT_CASE_EXTRA_CUSTOMERS')
OR col.object_id = OBJECT_ID('AT_CASE_PROPERTIES')
OR col.object_id = OBJECT_ID('AT_CASE_SCORING')
OR col.object_id = OBJECT_ID('AT_CASE_STATISTICS')
OR col.object_id = OBJECT_ID('AT_CASES')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_ADDRESSES')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_CONTACTS')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_CORE')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_EXTRA')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_INTERNET_IDS')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_MEMOS')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_PHONES')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_SCORING')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_STATISTICS')
OR col.object_id = OBJECT_ID('AT_CUSTOMER_STATISTICS_BU')
OR col.object_id = OBJECT_ID('AT_CUSTOMERS')
OR col.object_id = OBJECT_ID('AT_CUSTOMERS_EXTENSION')
OR col.object_id = OBJECT_ID('AT_DEBT_ELEMENTS')
OR col.object_id = OBJECT_ID('AT_DEBT_ELEMENTS_GROUPING')
OR col.object_id = OBJECT_ID('AT_DEBT_ELEMENTS_GROUPING_MEMBERS')
OR col.object_id = OBJECT_ID('AT_DEBT_ITEM_MEMOS')
OR col.object_id = OBJECT_ID('AT_DEBT_ITEMS')
OR col.object_id = OBJECT_ID('AT_DEBT_ITEMS_EXTENSION')
OR col.object_id = OBJECT_ID('AT_DEBTS')
OR col.object_id = OBJECT_ID('AT_DEBTS_EXTENSION')
OR col.object_id = OBJECT_ID('AT_DEBTS_STATISTICS')
OR col.object_id = OBJECT_ID('AT_MOVED_CASES_APPLICATION')
OR col.object_id = OBJECT_ID('AT_MOVED_CASES_EXTRA')
OR col.object_id = OBJECT_ID('AT_MOVED_CASES_HOST')
OR col.object_id = OBJECT_ID('AT_MOVED_CASES_RESTORE_HISTORY')
OR col.object_id = OBJECT_ID('AT_MOVED_CASES_STATISTIC')

order by tab.tab_name, col.column_id</CommandText>
